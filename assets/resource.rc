#include <windows.h>

// application icon
1 ICON "icon.ico"

// version information
1 VERSIONINFO
FILEVERSION 39,01,0,0
PRODUCTVERSION 39,01,0,0
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
