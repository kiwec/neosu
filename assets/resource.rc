        BLOCK "040904B0"  // US English, Unicode
        BEGIN
            VALUE "CompanyName", "neosu"
            VALUE "FileDescription", "cookie clicker with extra features"
            VALUE "FileVersion", "39.01.0.0"
            VALUE "InternalName", "neosu"
            VALUE "LegalCopyright", "Copyright (C) 2025"
            VALUE "OriginalFilename", "neosu.exe"
            VALUE "ProductName", "neosu"
            VALUE "ProductVersion", "39.01"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200  // US English, Unicode
    END
END

// application manifest
1 24 "neosu.manifest"
